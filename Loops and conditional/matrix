dim=input("Matrix dimension as rowXcolumn:\n")
l=dim.split("X")
for i in range(int(l[0])):
    for j in range(int(l[1])):
        print(i*j,end=" ")
    print("\n")
li=[[0 for x in range(int(l[1]))] for x in range(int(l[0]))]
for i in range(int(l[0])):
    for j in range(int(l[1])):
        li[i][j]=i*j

print(li)
